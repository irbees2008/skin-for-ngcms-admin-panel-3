<tr>
	<td><input type="checkbox" name="files[]" value="{id}"></td>
	<td>{insert_file}</td>
	<td>{insert_thumb}</td>
	<td>{insert_preview}</td>
	[preview]<td>{preview_img} {preview_size}</td>[/preview]
	<td><a href="{edit_link}" title="{l_edit}">{file_name}</a></td>
	<td>{view_file}</td>
	<td>{view_thumb}</td>
	<td>{width}x{height}</td>
	<td nowrap="nowrap">{size}</td>
	<td>{folder}</td>
	<td>{user}</td>
</tr>